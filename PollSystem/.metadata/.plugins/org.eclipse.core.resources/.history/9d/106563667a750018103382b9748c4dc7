package account;

public class memberGift {
	private String gift;
	private int gift_amount=0;
	
	public memberGift (String gift) {
		this.set_gift(gift);
	}
	
	public void set_gift(String gift) {
		this.gift= gift;
		this.gift_amount+=1;
	}
	
	public void add_gift() {
		gift_amount+=1;
	}
	public String get_gift() {
		return this.gift;
	}
}
